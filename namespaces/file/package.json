		"@-/errors": "workspace:*",
		"@-/user": "workspace:*",
		"@-/zod": "workspace:*",
		"corvex": "0.0.42",
		"react": "*",
		"swr": "2.2.5"
	},
	"publishable": {
		".": true,
		"./react": true,
		"./selections": true,
		"./tables": true,
		"./typeguards": true
	}
